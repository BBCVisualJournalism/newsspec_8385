                <div class="NMR_map-key-title cm-map-key-title"><%= nmr_map_key_text_1 %></div>

                <div class="cm-controls-vocabs" data-play="<%= play %>" data-pause="<%= pause %>" data-reset="<%= reset %>"></div>
            </div>

            <div class="cm-comparison-container">

                <h3><%= u5mr_chart_title %></h3>
                <p><%= u5mr_chart_subheader_1 %></p>
