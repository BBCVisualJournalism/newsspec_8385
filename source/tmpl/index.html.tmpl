                <h2 class="cm-year-title">2013</h2>

                <div class="holding-graphic">
                    <!-- p style="color: white; position: absolute; margin: 1em;">iFS 1.4</p-->
                    <img class="non-js" src="<%= pathStatic %>/<%= vocab_dir %>/img/308/u5mr_map_<%= vocab_dir %>.gif" alt="<%=  u5mr_2013_map_alt_text%>" />
                    <!-- img class="non-js" src="<%= pathStatic %>/<%= vocab_dir %>/img/308/nmr_map_<%= vocab_dir %>.gif" alt="<%=  nmr_2013_map_alt_text %>" /-->
